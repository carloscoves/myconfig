[user]
	email = carlos.coves@ext.aqualogybs.net
	name = Carlos Coves Prieto
[core]
	autocrlf = input
	safecrlf = true
	excludesfile = ~/.gitignore_global
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    type = cat-file -t
    dump = cat-file -p

[color]
  branch = auto
  diff = auto
  status = auto

[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green

[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold

[color "status"]
  added = green
  changed = red
  untracked = cyan
[push]
	default = simple
#[branch]
#	autosetuprebase = always
[credential]
	helper = cache --timeout=43200
